{
	"name": "pros",
	"displayName": "pros",
	"description": "Open Source C/C++ Development for VEX EDR",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/project.pros"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pros.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "pros.upload",
				"title": "Upload PROS Project",
				"icon": "$(live-share)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "pros-view-container",
					"title": "PROS",
					"icon": "media/pros-tux-white.svg"
				}
			]
		},
		"views": {
			"pros-view-container": [
				{
					"id": "prosTreeview",
					"name": "PROS",
					"icon": "media/pros-tux-white.svg",
					"contextualTitle": "PROS"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "pros.upload",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm-run-all compile:*",
		"compile:extension": "tsc -watch -p ./",
		"compile:views": "webpack --mode development",
		"watch": "npm-run-all -p watch:*",
		"watch:extension": "tsc -watch -p ./",
		"watch:views": "webpack --watch --mode development",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"css-loader": "^4.3.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"npm-run-all": "^4.1.5",
		"style-loader": "^1.2.1",
		"ts-loader": "^8.0.3",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@purduesigbots/pros-cli-middleware": "^2.5.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	}
}
